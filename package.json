{
  "name": "@bringbeyond/lb4-files-extension",
  "version": "0.2.1",
  "description": "lb4 files extension",
  "keywords": [
    "loopback-extension",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BringBeyond/lb4-files-extension"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "author": "julian-al-03 <julian.albers@bahrrauhkirschning.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/build": "^9.0.1",
    "@loopback/repository": "^5.0.1",
    "@loopback/rest": "^12.0.1",
    "@loopback/testlab": "^5.0.1"
  },
  "dependencies": {
    "@BringBeyond/lb4-base-extension": "^0.9.0",
    "@azure/storage-blob": "^12.10.0",
    "@loopback/boot": "^5.0.1",
    "@loopback/context": "^5.0.1",
    "@loopback/core": "^4.0.1",
    "@loopback/eslint-config": "^12.0.3",
    "eslint": "^8.10.0",
    "loopback-datasource-juggler": "^4.27.1",
    "loopback4-authorization": "^5.0.7",
    "multer": "^1.4.5-lts.1",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@loopback/boot": "^5.0.1",
    "@loopback/build": "^9.0.1",
    "@loopback/context": "^5.0.1",
    "@loopback/core": "^4.0.1",
    "@loopback/eslint-config": "^12.0.3",
    "@loopback/repository": "^5.0.1",
    "@loopback/rest": "^12.0.1",
    "@loopback/testlab": "^5.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^12.20.46",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.10.0",
    "loopback-datasource-juggler": "^4.27.1",
    "source-map-support": "^0.5.21",
    "tslib": "^2.0.0",
    "typescript": "~4.5.5"
  }
}
